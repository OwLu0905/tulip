---
import Header from "@components/Header.astro";
import BaseHead from "./BaseHead.astro";

type Props = {
  title: string;
};
const { title = "Tulip" } = Astro.props;
---

<html lang="en">
  <BaseHead title={title} />
  <body>
    <Header />
    <main class="container">
      <slot />
    </main>
  </body>
</html>
